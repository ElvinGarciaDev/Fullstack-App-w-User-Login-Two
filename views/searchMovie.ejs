    <title>Search For A Movie</title>
    <link rel="stylesheet" href="searchMovie.css">

</head>
<body>


    <section class="pt-5 topSection">
        <div class="container text-center">
            <h1>Search for a Movie</h1>
            <span>Add it to your bookmarks by clicking the star</span>
            <a class="btn btn-outline-secondary" role="button" id="profile" href="profile">Profile</a>

            <div class="input-group mb-3 pt-2">
                <input type="text" class="form-control" placeholder="Movie Name">
                <button class="btn btn-outline-secondary" type="button" id="button-addon2">Search</button>
            </div>
        </div>
    </section>
    

    <!--DISPLAY MOVIES-->

    <section>
        <div class="container">

            <div class="row text-center g-0" id="displayMovies">

            </div>

        </div>

    </section>

    <script src="searchMovie.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
